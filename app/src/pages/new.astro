---
import DashboardLayout from "../layouts/DashboardLayout.astro";
import isLoggedIn from "../utils/isLogged";
import CreateProjectForm from "../components/Forms/CreateProjectForm";
const jwt = Astro.request.headers.get('cookie')
if (!await isLoggedIn(jwt || '')) return await Astro.redirect('/dashboard')

---

<DashboardLayout>
    <div class="mt-24 justify-center flex ">
        <CreateProjectForm client:load />
    </div>
</DashboardLayout>